                android:layout_marginEnd="176dp" android:layout_marginRight="176dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="177dp"
                android:layout_marginStart="177dp"
                style="@style/Widget.AppCompat.AutoCompleteTextView"
                android:visibility="visible" tools:visibility="visible"
                android:textAppearance="@style/TextAppearance.AppCompat.Display1"
                app:layout_constraintHorizontal_bias="0.504" app:layout_constraintVertical_bias="0.0"/>
        <ImageView
                android:layout_width="132dp"
                android:layout_height="127dp" app:srcCompat="@android:drawable/arrow_down_float"
