        />
        <TextView
                android:text='"Current Playlist"'
                android:layout_width="264dp"
                android:layout_height="73dp"
                android:textColor="#1BD954"
                android:background="#191414"
                android:id="@+id/textView8" android:layout_marginTop="162dp"
