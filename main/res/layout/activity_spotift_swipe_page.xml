                android:layout_height="80dp" android:id="@+id/textView6"
                android:textSize="40dp"
                android:background="#191414"
                android:textColor="#1BD954"
                style="@style/Widget.AppCompat.AutoCompleteTextView"
                android:layout_marginStart="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toEndOf="@+id/textView5" android:layout_marginRight="8dp"
                app:layout_constraintHorizontal_bias="0.707" app:layout_constraintTop_toBottomOf="@+id/textView5"
