                android:text='"Artist"'
                android:layout_width="150dp"
                android:layout_height="80dp"
                android:id="@+id/textView7" android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toTopOf="@+id/imageView2"
                android:textSize="40dp"
                android:textColor="#1BD954"
                android:background="#191414"
                style="@style/Widget.AppCompat.AutoCompleteTextView"
                android:layout_marginStart="56dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="56dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
                app:layout_constraintHorizontal_bias="0.375" app:layout_constraintTop_toBottomOf="@+id/textView6"
